{
  "name": "rambdax",
  "version": "2.13.1",
  "description": "Lightweight alternative to Ramda - extended version",
  "main": "./dist/rambdax.js",
  "module": "./dist/rambdax.esm.js",
  "umd": "./dist/rambdax.umd.js",
  "typings": "./index.d.ts",
  "sideEffects": false,
  "scripts": {
    "docz:dev": "DOCZ_SOURCE=doc_files docz dev",
    "docz:build": "docz build",
    "sync": "yarn sync:work&&yarn sync:after",
    "sync:after": "rm -rf -d src/rambda/*.spec.js",
    "sync:work": "node files/_modules/sync",
    "do": "node files/_modules/do",
    "readme": "node files/_modules/createReadme",
    "docs": "docsify init ./docs",
    "test": "jest src",
    "jest": "jest --watch -o",
    "cover": "jest src --coverage",
    "build:main": "rollup -c files/rollup.config.js",
    "build:web": "rollup -c files/rollup.web.config.js",
    "build": "yarn build:main&&yarn build:web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/selfrefactor/rambdax.git"
  },
  "keywords": [
    "rambda",
    "lodash",
    "ramda",
    "util"
  ],
  "author": "self_refactor",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/selfrefactor/rambdax/issues"
  },
  "homepage": "https://github.com/selfrefactor/rambdax#readme",
  "dependencies": {},
  "devDependencies": {
    "fs-extra": "^7.0.0",
    "string-fn": "^2.0.0",
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@types/jest": "24.0.17",
    "docz": "^1.2.0",
    "docz-theme-default": "^1.2.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "babel-jest": "24.9.0",
    "docsify-cli": "4.3.0",
    "helpers": "https://github.com/selfrefactor/helpers#1.6.0",
    "jest": "24.9.0",
    "rambdax": "2.13.0",
    "ramda": "0.26.1",
    "rollup": "1.19.4",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-cleanup": "3.1.1",
    "rollup-plugin-commonjs": "10.0.2",
    "rollup-plugin-json": "3.1.0",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-replace": "2.1.0",
    "rollup-plugin-sourcemaps": "0.4.2"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/*.js",
      "!src/shuffle.*",
      "!_internals"
    ]
  },
  "files": [
    "dist",
    "src",
    "index.d.ts"
  ],
  "commitMessage": {
    "feature": [
      "method",
      "sync",
      "deprecate"
    ],
    "support": [
      "build",
      "lint",
      "codestyle",
      "clean",
      "prepublish",
      "typescript",
      "sync",
      "todo",
      "script"
    ],
    "docs": [
      "method",
      "build",
      "docsify",
      "changelog"
    ],
    "test": [
      "method"
    ],
    "typings": [
      "rambda",
      "fix"
    ],
    "workInProgress": "method component to show method tests and docs"
  }
}